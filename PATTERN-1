// Enter an number5
// *	*	*	*	*	
// *	*	*	*	*	
// *	*	*	*	*	
// *	*	*	*	*	
// *	*	*	*	*	

#include <stdio.h>
int main()
{
    int n;
    printf("Enter an number");
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            printf("*\t");
        }
        printf("\n");
    }
    return 0;
}
