#include <stdio.h>
void swap(int* a,int* b)
{
    int temp=*a;
    *a=*b;
   *b=temp;
    //printf("After Swapping %d %d:\n",*a,*b);
}
int main(){
   
    int a,b;
    printf("Enter Number a,b :");
    scanf("%d %d",&a,&b);
    printf("Before Swapping %d %d:\n",a,b);
    swap(&a,&b);
    printf("After Swapping %d %d:\n",a,b);
   
}   
