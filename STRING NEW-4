#include <stdio.h>
void convert()
{
     char str[50];
     scanf("%s",str);
     int l;
    for(int i=0;str[i]!='\0';i++)
    {
        if(str[i]>='A'&&str[i]<='Z')
        {
            str[i]=str[i]+32;
        }
       l=i;
    }
    if(str[l]>='a'&&str[l]<='z')
    {
        str[l]=str[l]-32;
    }
    printf("%s",str);
    
}
int main(){
    
    convert();
}    
