#include <stdio.h>
struct employee
{
    int id;
    char *name[20];
    long int phone;
};
int main()
{
    struct employee e1;
    printf("Enter the Employee ID");
    scanf("%d",&e1.id);
    printf("Enter the Employee phone number");
    scanf("%ld",&e1.phone);
    printf("Enter Employee name");
    scanf(" %[^\n]s",e1.name);
    printf("%d\n",e1.id);
    printf("%ld\n",e1.phone);
    printf("%s",e1.name);
}
